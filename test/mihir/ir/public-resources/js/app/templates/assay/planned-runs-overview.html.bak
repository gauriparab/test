            <span class="vpad14" >{{t 'planned.runs.selectedRuns'}}:<span class="selected-samples" id="plannedRunsCount">{{t 'manage.sample.selectedcount'}}</span></span>
        </div>
    </div>

    <div class="row-fluid">
