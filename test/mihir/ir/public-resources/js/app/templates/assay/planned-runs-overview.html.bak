<div class="container-fluid">
    <div class="row-fluid">
        <div class="pull-left">
            <h1><img src="{{img 'dash-workflows.png'}}" width="40" /> {{t 'planned.runs.title'}}</h1>
        </div>
    </div>

    <div class="row-fluid ">
        <div class="actionSearch">
