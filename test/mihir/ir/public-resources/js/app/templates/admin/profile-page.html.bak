<div class="container-fluid">
	<div class="row-fluid">
	<div class="pull-left">
			<h1>
				My Profile
			</h1>
	</div>
	</div>
	<div class="row-fluid ">
		<div class="span12">
		<table cellpadding="10" width="100%">
			<tbody>
				<tr>
					<td><strong>User Name:</strong></td>
					<td>{{user.userName}}</td>
				</tr>
				<tr>
					<td><strong>Password:</strong></td>
					<td>**********<a href="#" id="changePassword">Change Password</a></td>
				</tr>			
				<tr>
					<td colspan="2" class="greyHeading"><strong> Account Information</strong></td>
				</tr>
				<tr>
					<td><strong>First Name:</strong></td>
					<td>{{user.firstName}}</td>
				</tr>
				<tr>
					<td><strong>Last Name:</strong></td>
					<td>{{user.lastName}}</td>
				</tr>
				<tr>
					<td><strong>Email:</strong></td>
					<td>{{user.email}} <a href="#" id="changeEmail">Change Email</a></td>
				</tr>
				<tr>
					<td><strong>Status:</strong></td>
					<td>{{user.state}}</td>
				</tr>
				<tr>
					<td><strong>Role:</strong></td>
					<td>{{user.roleDto.roleName}}</td>
				</tr>
				<tr>
					<td colspan="2" class="greyHeading"><strong> Dates</strong></td>
				</tr>
				<tr>
					<td><strong>Created On:</strong></td>
					<td>{{user.createdOn}}</td>
				</tr>
				<tr>
					<td><strong>Last Login:</strong></td>
					<td>{{user.lastLoginDate}}</td>
				</tr>
				
			</tbody>
		</table>
	</div>
	</div>
	
	<div class="pull-left">
            <a href="javascript:history.back()" class="btn">< Back</a>
    </div>
	 <div class="modal hide fade" id="manageUserModal" tabindex="-1" role="dialog" aria-labelledby="createUserModalLabel" aria-hidden="true">
     </div>
</div>
